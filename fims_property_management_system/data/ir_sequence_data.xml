<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for lead mining requests -->
        <record id="seq_property_details" model="ir.sequence">
            <field name="name">Property Details</field>
            <field name="code">property.details</field>
            <field name="prefix">P/</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_property_sales" model="ir.sequence">
            <field name="name">Property Sales</field>
            <field name="code">property.sales</field>
            <field name="prefix">PS/</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_tenant_details" model="ir.sequence">
            <field name="name">Tenant Details</field>
            <field name="code">tenant.details</field>
            <field name="prefix">T/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
